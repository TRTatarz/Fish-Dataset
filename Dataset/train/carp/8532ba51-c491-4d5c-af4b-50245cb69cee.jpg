      <form class="weather-widget-form no-weather hide-formatting noaa-weather-widget-form" action="#" method="post" accept-charset="UTF-8">
  <div class="upper">
    <script class="noaa_weather_upper" type="text/x-handlebars-template" >
      
        {{#if nodata}}
          <div class="current-temp"></div>
        {{else}}
          <div class="current-temp" {{#if icon}}style="background-image: url(&quot;/themes/custom/noaa/images/icons/weather/{{icon}}&quot;)" {{/if}}>{{#if deg}}{{deg}}°{{/if}}</div>
        {{/if}}

        <div class="inner">
          <div class="city icon-location-after">{{#if nodata}}Not Available{{else}}{{city}}{{#if state}}, {{state}}{{/if}}{{/if}}</div>
        </div>
      
    </script>
    <div class="weather-toggle icon-location-after" tabindex="0">
      <span class="underline">Find your local weather</span>
    </div>
  </div>
  <div class="lower">
    <div class="close-button" tabindex="0"></div>
    <script class="noaa_weather_lower" type="text/x-handlebars-template" >
      
        <div class="city">{{#if nodata}}Not Available{{else}}{{city}}{{#if state}}, {{state}}{{/if}}{{/if}}</div>
        <div class="forecast-wrapper{{#if pop}} has-precip{{/if}}">
          <div class="upper">
          <div class="inner">
            {{#if nodata}}
              <div class="current-temp"></div>
            {{else}}
              <div class="current-temp" {{#if icon}}style="background-image: url(&quot;/themes/custom/noaa/images/icons/weather/{{icon}}&quot;)" {{/if}}>{{#if deg}}{{deg}}°{{/if}}</div>
              <div class="label">currently</div>
            {{/if}}
          </div>
          </div>
          {{#if nodata}}
            <a class="not-available" href="/no-data">Why is data not available?</a>
          {{else}}
          <div class="forecast-temp forecast-item"><div class="data">{{maxt}}/{{mint}}° </div><div class="label">high/low</div></div>
          {{#if pop}}<div class="forecast-precip forecast-item"><div class="data">{{pop}}% </div><div class="label">chance of {{precip_type}}</div></div>{{/if}}
          {{#if wspd}}<div class="wind forecast-item"><div class="data">{{#if wdir}}{{wdir}}{{/if}} {{wspd}} </div><div class="label">mph winds</div></div>{{/if}}
        </div>
        <a href="http://forecast.weather.gov/MapClick.php?lat={{lat}}&amp;lon={{lng}}" class="forecast-link{{#if pop}} has-precip{{/if}}" name="edit-forecast">See full local forecast</a>
        {{/if}}
      
    </script>
    <div class="form-wrapper">
